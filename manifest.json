
{
  "manifest_version": 2,

  "name": "Repl.it Themes",
  "description": "Make Repl.it look better",
  "version": "1.0",

  "content_scripts": [
   {
     "matches": ["https://repl.it/*"],
     "js": ["index.js"]
   }
 ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "permissions": [
    "activeTab",
    "storage",
    "*://*/*"
  ]
}
